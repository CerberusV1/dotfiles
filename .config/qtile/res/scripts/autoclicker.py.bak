import time
from pynput.mouse import Button, Controller

mouse = Controller()

try:
    while True:
        mouse.click(Button.right, 1)  # Simulates left-click
        mouse.click(Button.right, 1)  # Simulates left-click
        time.sleep(0.2)  # Adjust the interval
except KeyboardInterrupt:
    print("Autoclicker stopped.")
